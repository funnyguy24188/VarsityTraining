<section id="mu-features">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 col-md-12">
        <div class="mu-features-area">
          <!-- Start Title -->
          <div class="mu-title">
            {% if header %}
              <header>
                {{ header }}
              </header>
            {% endif %}
          </div>
          <!-- End Title -->
          <!-- Start features content -->
          <div class="mu-features-content">
            <div class="row">
              {{ rows }}
            </div>
          </div>
          <!-- End features content -->
          {% if footer %}
            <footer>
              {{ footer }}
            </footer>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</section>
